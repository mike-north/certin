{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.enable": true,
  "eslint.workingDirectories": [{ "pattern": "./packages/*/" }],
  "files.associations": { "*.json": "jsonc" },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/dist": true,
    "**/temp": true,
    "**/.private-types": true,
    "**/tests_js": true,
    "**/node_modules": true,
    "**/.DS_Store": true,
    "packages/*/tests_js": true,
    "**/tsconfig.tsbuildinfo": true
  }
}
